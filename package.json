{
  "name": "ts-souko",
  "version": "0.1.0",
  "license": "MIT",
  "author": "jiftechnify",
  "main": "dist/index.js",
  "module": "dist/ts-souko.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/ts-souko.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ts-souko.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^0.20.0",
    "husky": "^7.0.4",
    "size-limit": "^7.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.3"
  }
}
